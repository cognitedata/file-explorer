{
  "name": "digital-twin-explorer",
  "description": "Review tool for 3D models",
  "version": "0.1.0",
  "author": "Fredrik Anfinsen",
  "license": "Apache-2.0",
  "dependencies": {
    "@cognite/gearbox": "^0.0.45",
    "@cognite/griff-react": "^0.5.0-rc.56",
    "@cognite/sdk": "2.0.0-beta.1",
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "antd": "^3.18.1",
    "prop-types": "^15.6.2",
    "query-string": "^6.5.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-moment": "^0.9.2",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-router-util": "^0.8.0",
    "react-scripts": "^2.1.8",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@cognite/eslint-config": "^1.1.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-redux": "^3.0.1",
    "husky": "^0.14.3",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0"
  },
  "scripts": {
    "start": "env HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --write '**/*.{js,jsx,json,md}'",
    "format:changed": "pretty-quick",
    "format:staged": "pretty-quick --staged",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint 'src/**/*.js' --quiet",
    "lint-fix": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "npm": ">=4",
    "node": ">=6"
  }
}
